<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/obtenerMenuLlprincipal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".ReservarFragment" >

    <EditText
        android:id="@+id/obtenerMenuEtFecha"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/obtener_menu_fecha" >


    </EditText>

    <Spinner
        android:id="@+id/obtenerMenuSpTipoMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/obtenerMenuBtVerMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/obtener_menu_ver_menu" />

</LinearLayout>
