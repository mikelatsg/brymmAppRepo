<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/itemTipoArticuloTvTipoArticulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/itemTipoArticuloTvPrecio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/item_tipo_articulo_precio" />

    <CheckBox
        android:id="@+id/itemTipoArticuloCbPersonalizar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/item_tipo_articulo_personalizar" />

</LinearLayout>